#!groovy
//Run NGINX docker build
properties([dosableConcurrentBuilds()])

pipeline {
	agent {
		label 'master'
		}
	options {
		buildDiscarder (logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
		timestamps()
	}
	stages {
		stage("create docker image") {
			steps {
				echo "========= start build image =========="
				dir ('docker/NGINX') {
					sh 'docker build .'
				}			
			}
		}
	}
}
